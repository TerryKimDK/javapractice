package app;

class X {
	protected int i = 10;
	protected String msg = "I am an X.";
	
	public void play() { 
		System.out.println("Play.. " + msg);
	}
}

Class Y extends X {
	protected int i = 20;
	protected String msg = "I am an Y.";
	@Override
	public void print() {
		System.out.println(msg);
	}
}


public class Z {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
